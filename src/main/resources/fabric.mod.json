{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "icon": "assets/litematica-printer/icon.png",
  "name": "${mod_name}",
  "description": "Litematica的一个附属模组，增加了缺失的打印功能",
  "authors": [
    "aleksilassila",
    "zhaixianyu",
    "BlinkWhite"
  ],
  "contact": {
    "website": "${mod_homepage}",
    "homepage": "${mod_sources}",
    "sources": "${mod_sources}"
  },

  "license": "${mod_license}",

  "environment": "*",
  "entrypoints": {
    "main": [
      "me.aleksilassila.litematica.printer.LitematicaMixinMod"
    ],
    "client": [
      "me.aleksilassila.litematica.printer.LitematicaMixinMod"
    ],
    "modmenu": [
      "me.aleksilassila.litematica.printer.config.ModMenu"
    ]
  },
  "mixins": [
    "litematica-printer.mixins.json"
  ],

  "depends": {
    "minecraft": "${minecraft_dependency}"
  },
  "custom": {
    "modmenu": {
      "parent": "${mod_id}-wrapper"
    }
  }
}

